<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Home.EditProfileActivity">
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
  >

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="20dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Name"
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="Full Name"
        android:padding="12dp"
        android:textColor="#615454"

        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="DOB "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/dob"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="DOB"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Email "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="Email"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Phone Number "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/phone_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="Phone"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="CNIC "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/cnic_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="CNIC"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="25dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Social Links"
        android:textSize="25dp" />

    <ImageView
        android:id="@+id/edit_lyt"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentRight="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="25dp"
        android:src="@drawable/outline_edit_location_alt_24" />
  </RelativeLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="20dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Facebook"
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/facebook"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#615454"

        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Twitter "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/twitter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Instagram "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/instagram"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Reddit "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/reddit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Pinterest "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/pinterest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="25dp"
      android:layout_marginTop="10dp"
      android:layout_marginRight="25dp"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:hint="Linked in "
        android:padding="12dp"
        android:textColor="@color/textColor"
        android:textColorHint="@color/textColor"
        android:textSize="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/linked_in"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/poppins_regular"
        android:hint="not added yet"
        android:padding="12dp"
        android:textColor="#6E6B6B"
        android:textSize="15dp" />


  </LinearLayout>


</LinearLayout>
  </androidx.core.widget.NestedScrollView>